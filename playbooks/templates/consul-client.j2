datacenter = "rtlab"
data_dir = "/opt/consul"
encrypt = "{{ encrypt_key }}"
advertise_addr = "{{ ansible_ens3.ipv4.address }}"
retry_join = ["consul1.gh40.lab", "consul2.gh40.lab", "consul3.gh40.lab"]
acl = {
    enabled = true,
    default_policy = "allow",
    enable_token_persistence = true
    tokens = {
      "master" =  "{{ acl_token }}"
    }
}
